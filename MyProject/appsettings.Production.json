{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "bbc3dff8-b0ef-4e23-a5f3-6b59be3987b7",
        "SanitizeTinyMce": true,
        "InstallMissingDatabase": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "MacroErrors": "Silent"
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "Hosting": {
        "Debug": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=${DATABASE_HOST};Database=${DATABASE_NAME};User Id=${DATABASE_USER};Password=${DATABASE_PASSWORD};TrustServerCertificate=true;Encrypt=false;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}